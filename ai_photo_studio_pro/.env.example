# AI Photo Studio Pro - Flutter App Environment Variables
# Copy this file to .env and fill in your actual values

# ============================================================================
# SUPABASE CONFIGURATION
# ============================================================================
# Get these from your Supabase project settings:
# https://app.supabase.com/project/_/settings/api

# Supabase Project URL
SUPABASE_URL=https://your-project-id.supabase.co

# Supabase Anon/Public Key (safe to use in client apps)
SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6InlvdXItcHJvamVjdC1pZCIsInJvbGUiOiJhbm9uIiwiaWF0IjoxNjc4ODg4ODg4LCJleHAiOjE5OTQ0NjQ4ODh9.xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Environment-specific Supabase Configuration
DEV_SUPABASE_URL=https://your-dev-project.supabase.co
DEV_SUPABASE_ANON_KEY=your-dev-supabase-anon-key-here
PROD_SUPABASE_URL=https://your-prod-project.supabase.co
PROD_SUPABASE_ANON_KEY=your-prod-supabase-anon-key-here

# ============================================================================
# AI MODEL CONFIGURATION
# ============================================================================
# Gemini API Configuration (for existing features)
GEMINI_API_KEY=your-gemini-api-key-here

# ============================================================================
# REVENUECAT CONFIGURATION
# ============================================================================
# Get your API keys from: https://app.revenuecat.com/

# RevenueCat API Key (Public)
REVENUECAT_API_KEY=your_revenuecat_public_api_key

# RevenueCat App-specific API keys (optional, platform-specific)
REVENUECAT_APPLE_API_KEY=appl_xxxxxxxxxxxxxxxxxxxx
REVENUECAT_GOOGLE_API_KEY=goog_xxxxxxxxxxxxxxxxxxxx

# ============================================================================
# PRODUCT IDS (In-App Purchases)
# ============================================================================
# These should match your RevenueCat product identifiers

# Monthly Subscription
PRODUCT_ID_MONTHLY=ai_photo_studio_pro_monthly

# Yearly Subscription
PRODUCT_ID_YEARLY=ai_photo_studio_pro_yearly

# Lifetime Purchase
PRODUCT_ID_LIFETIME=ai_photo_studio_pro_lifetime

# ============================================================================
# ENTITLEMENT IDS (RevenueCat)
# ============================================================================
# Premium entitlement identifier
ENTITLEMENT_ID_PRO=pro

# ============================================================================
# ANALYTICS & MONITORING (Optional)
# ============================================================================
# Firebase (if using)
# FIREBASE_API_KEY=
# FIREBASE_PROJECT_ID=
# FIREBASE_APP_ID=

# Sentry (Error Tracking)
# SENTRY_DSN=https://xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx@sentry.io/xxxxxxx

# Mixpanel (Analytics)
# MIXPANEL_TOKEN=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# ============================================================================
# APP CONFIGURATION
# ============================================================================
# App Environment (development, staging, production)
APP_ENV=development

# Enable debug mode
DEBUG_MODE=true

# Enable verbose logging
ENABLE_LOGGING=true

# ============================================================================
# ENHANCED AI FEATURES (New)
# ============================================================================
# Enable AI Retouch Feature
ENABLE_AI_RETOUCH=true

# Enable Background Editing Feature
ENABLE_BACKGROUND_EDITING=true

# Enable Outfit Customization Feature
ENABLE_OUTFIT_CUSTOMIZATION=true

# Enable Aspect Ratio Expansion Feature
ENABLE_ASPECT_EXPANSION=true

# Enable 4K Upscaling Feature
ENABLE_4K_UPSCALING=true

# Enable Batch Generation Feature
ENABLE_BATCH_GENERATION=true

# ============================================================================
# API ENDPOINTS (Supabase Edge Functions)
# ============================================================================
# These are automatically constructed from SUPABASE_URL
# Edge Function URLs format: {SUPABASE_URL}/functions/v1/{function-name}

# AI Photo Enhancement Function
EDGE_FUNCTION_PHOTO_ENHANCE=/functions/v1/ai-photo-enhance

# Background Removal Function
EDGE_FUNCTION_BACKGROUND_REMOVAL=/functions/v1/background-removal

# Image Upscaling Function
EDGE_FUNCTION_IMAGE_UPSCALING=/functions/v1/image-upscaling

# Image Expansion Function
EDGE_FUNCTION_IMAGE_EXPANSION=/functions/v1/image-expansion

# Outfit Change Function
EDGE_FUNCTION_OUTFIT_CHANGE=/functions/v1/outfit-change

# ============================================================================
# RATE LIMITING & QUOTAS
# ============================================================================
# Free tier quotas
FREE_TIER_DAILY_GENERATIONS=3
FREE_TIER_STYLES_COUNT=5

# Pro tier quotas
PRO_TIER_MONTHLY_GENERATIONS=100
PRO_TIER_STYLES_COUNT=50

# Maximum photos in batch generation
MAX_BATCH_PHOTOS=100

# Maximum variations per photo
MAX_VARIATIONS_PER_PHOTO=20

# ============================================================================
# STORAGE & CACHING
# ============================================================================
# Local storage path for cached images
LOCAL_CACHE_PATH=./cache/images

# Maximum cache size in MB
MAX_CACHE_SIZE_MB=500

# Cache expiration time in days
CACHE_EXPIRATION_DAYS=7

# ============================================================================
# UI CONFIGURATION
# ============================================================================
# Default language (en, ru, uk)
DEFAULT_LANGUAGE=en

# Enable dark mode by default
DEFAULT_DARK_MODE=false

# Show onboarding on first launch
SHOW_ONBOARDING=true

# ============================================================================
# SUPPORT & LEGAL
# ============================================================================
# Support email
SUPPORT_EMAIL=support@aiphotostudiopro.com

# Terms of Service URL
TERMS_OF_SERVICE_URL=https://aiphotostudiopro.com/terms

# Privacy Policy URL
PRIVACY_POLICY_URL=https://aiphotostudiopro.com/privacy

# ============================================================================
# DEVELOPMENT SETTINGS
# ============================================================================
# Enable mock API responses (for testing without backend)
ENABLE_MOCK_API=false

# Skip subscription checks (for testing)
SKIP_SUBSCRIPTION_CHECK=false

# Enable all premium features (for testing)
UNLOCK_ALL_FEATURES=false
